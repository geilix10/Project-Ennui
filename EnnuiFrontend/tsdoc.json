{
	"tags":
	{
		"allowUnknownTags" : true
	},
	"plugins":[
		 "plugins/markdown"
		,"/usr/local/lib/node_modules/tsdoc/template/plugins/TSDoc.js"
	],
	"opts":
	{
		"template"	:"/usr/local/lib/node_modules/tsdoc/template",
		"recurse"	:"true"
	},
	"templates" : {
		"cleverLinks"     : false,
		"monospaceLinks"  : false
	},
	"source":
	{
		"includePattern": "(\\.d)?\\.ts$"
	},
	"markdown"  : {
		"parser"   : "gfm",
		"hardwrap" : true
	},
	"tsdoc":{
		"tutorials"		:"",
		"systemName"	: "was-tun",
		"footer"		: "",
		"copyright"		: "was-tun Copyright Â© 2016 Markus Geilehner.",
		"outputSourceFiles" : true,
		"commentsOnly": true
	}
}
